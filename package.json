{
  "name": "user-manager",
  "version": "1.0.0",
  "description": "This is a User manager micro-services for nodejs backend applications, backed by express server",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=default && nodemon --exec 'eslint ./ && babel-node server.js'",
    "start:dev": "export NODE_ENV=dev && node dist/server.js",
    "build": "babel src -d dist/src  && babel config -d dist/config && babel server.js -d dist",
    "start:prod": "export NODE_CONFIG_DIR=./dist/config && node dist/server.js",
    "pm2-start": "pm2 start node dist/server.js",
    "test": "mocha './src/**/*.test.js' --exit --require @babel/register",
    "test-with-coverage": "nyc --reporter=html --reporter=text mocha --recursive",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "backend",
    "server",
    "node",
    "service"
  ],
  "author": "Adrixus Studio Pvt. Ltd.",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.56.0",
    "@sentry/node": "^6.2.0",
    "aws-sdk": "^2.957.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "ioredis": "^4.28.5",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "kafka-node": "^5.0.0",
    "lodash": "^4.17.21",
    "mocha": "^8.0.1",
    "moment": "^2.29.1",
    "mongoose": "^5.9.22",
    "nodemailer": "^6.6.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "passport": "0.2.1",
    "passport-facebook": "latest",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.5.1",
    "safe-await": "^1.0.1",
    "sequelize": "^6.5.0",
    "socket.io": "^4.1.2",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0"
  }
}
